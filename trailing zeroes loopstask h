#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    // Write C code here
   int n,r=1;
   scanf("%d",&n);
   while(n>=1)
   {
       r=r*n;
       n=n-1;
   }
   
    int count=0,s;
   
        s=r%10;
        r=r/10;
        if(s==0)
        {
        count=count+1;
         printf("%d",count);
        }
    else
    {
        printf("0");
    }
    return 0;
}
