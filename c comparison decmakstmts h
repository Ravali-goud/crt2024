#include <stdio.h>

int main() {
    int t;
    scanf("%d", &t);

    while (t!=0) {
        int a, b;
        char op;

        // Read the input for a single test case
        scanf("%d %c %d", &a, &op, &b);

        // Perform the comparison based on the operator
        if ((op == '>' && a > b) || (op == '<' && a < b) || (op == '=' && a == b)) {
            printf("Right\n");
        } else {
            printf("Wrong\n");
        }
        t--;
    }

    return 0;
}
