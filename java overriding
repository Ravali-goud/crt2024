class Father
{
void surName()
{
System.out.println("K");
}
void designation()
{
System.out.println("Clerk");
}
}
class Child extends Father 
{
void getName()
{
System.out.println("Satish ");
}
void designation()
{
System.out.println("Java Programmer");
}
}
class Test4
{
public static void main(String[] args)
{
Child c1=new Child();
c1.getName();
c1.surName();
c1.designation();
}
}

