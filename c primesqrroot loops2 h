#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,count=0;
       scanf("%d",&n);
        for(int i=1;i<=n;i++)
        {
            if(n%i==0)
            {
                count++;
            }
        }
        if(count==2)
        {
           printf("%.2f",sqrt(n));
        }
        else
        {
      printf("%d is not a prime no",n);
        }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
