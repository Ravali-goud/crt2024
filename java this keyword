import java.util.*;
class Sample
{
int admno,mat,phy,che;
String name;
void get(int admno,String name,int mat,int phy,int che)
{
this.admno=admno;
this.name=name;
this.mat=mat;
this.phy=phy;
this.che=che;
}
void show()
{
System.out.println(this.admno+" "+this.name+" "+this.mat+" "+this.phy+" "+this.che);
}
}

class Test3
{
public static void main(String args[])
{
Sample s1=new Sample();
s1.get(111,"aaa",98,55,77);
s1.show();
Sample s2=new Sample();
s2.get(222,"kkk",88,78,33);
s2.show();
}
}
