import java.util.*;
class Test4
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
int m=s.nextInt();
int a1[]=new int[m];
for(int i=0;i<m;i++)
{
a1[i]=s.nextInt();
}
int p=m+n;
int k[]=new int[p];
int c=0;
for(int i=0;i<n;i++)
{
k[c]=a[i];
c++;
}
for(int i=0;i<m;i++)
{
k[c]=a1[i];
c++;
}
Arrays.sort(k);
for(int i=0;i<p;i++)
{
System.out.print(k[i]+" ");
}
}
}
