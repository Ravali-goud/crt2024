#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int n,m,i,j;
    scanf("%d%d",&n,&m);
    int a[n][m];
    for(i=0;i<n;i++)
    {
        for(j=0;j<m;j++)
        {
        scanf("%d",&a[i][j]);
        }
    }
    for(i=0;i<m;i++)
        {
            int colsum=0;
            for(j=0;j<n;j++)
                {
                    colsum=colsum+a[j][i];
                   
                } printf("%d\n",colsum);
    }   
    return 0;
}
