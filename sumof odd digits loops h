#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n,sum=0;
    scanf("%d",&n);
    while(n!=0)
    {
        int i=n%10;
        n=n/10;
        if(i%2!=0)
        {
          sum=sum+i;  
        }   
    } 
    printf("%d",sum);
    return 0;
