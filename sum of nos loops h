#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    int a,sum=0;
   scanf("%d",&a); 
   while(a!=-1)
   {
       sum=sum+a;
      scanf("%d",&a);
   }
     printf("%d",sum);
    return 0;
}
