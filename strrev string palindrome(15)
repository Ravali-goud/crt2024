#include<stdio.h>
#include<string.h>
int main()
{
	char str1[50],str2[50];
	int i,j=0,n;
	
	scanf("%s",str1);
	
	for(n=0;str1[n]!='\0';n++);{
	
	for(i=n-1;i>=0;i--)
	{
		str2[j]=str1[i];
		j++;
	}
}
	str2[j]='\0';
	
	i=strcmp(str1,str2);
	if(i==0)
	printf("palindrome");
	else
	printf("Not palindrome");
		
	}
