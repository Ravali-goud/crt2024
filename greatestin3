#include <stdio.h>
#include<math.h>

int main() {
    // Write C code here
    //greatest number among 3
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if(a>b&&a>c)
    {
        printf("%d",a);
    }
    else if(b>c)
    {
        printf("%d",b);
    }
    else
    {
        printf("%d",c);
    }
   
 
    return 0;
}
