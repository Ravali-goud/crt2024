#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int num,n,a;
    scanf("%d",&num);
        n=num%10;
        a=num/10;
    int k=pow(a,n);
    printf("%d",k);
    

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
