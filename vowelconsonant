#include <stdio.h>

int main() {
    // Write C code here
   
   //read a character to print the charcater is vowel or not
   char c;
   scanf("%c",&c);
   if(c=='a'||c=='e'||c=='i'||c=='o'||c=='u'||c=='A'||c=='E'||c=='I'||c=='O'||c=='U')
   {
       printf("vowel");
   }
   else
   {
       printf("consonant");
   }
    return 0;
}
