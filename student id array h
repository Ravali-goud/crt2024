#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int n;
    scanf("%d",&n);
    int i,a[n],j,t;
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]>a[j])
                {
                t=a[i];
                a[i]=a[j];
                a[j]=t;
                }
        }
    
    }
    for(i=0;i<n;i++)
    {
    if(i%2==0)
        printf("%d ",a[i]);
    
    }
    for(i=0;i<n;i++)
    {
        if(i%2!=0)
            printf("%d ",a[i]);
    }
    return 0;
}
