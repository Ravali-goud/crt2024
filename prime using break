// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int n,i,c=0;
    scanf("%d",&n);
    for(i=2;i<n;i++)
    {
        if(n%i==0)
        {
           c++;
           break;
        }
    }    
       
        if(c==0)
       printf("Prime number");
       else
       printf("not a prime number");
    
    

    return 0;
}
