import java.util.*;
class Test3
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
int n;
n=s.nextInt();
int a[]=new int[n];
for(int i=0;i<n;i++)
{
a[i]=s.nextInt();
}
int targetvalue=s.nextInt();
for(int i=0;i<n-1;i++)
{
for(int j=i+1;j<n;j++)
{
if(a[i]+a[j]==targetvalue)
{
System.out.println(i+" "+j);
return;
}
}
}

System.out.println(-1);
}
}
