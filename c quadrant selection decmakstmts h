#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() 
{
    int a,b;
    scanf("%d%d",&a,&b);
    if(a!=0&&b!=0)
    {
        if(a>0&&b>0)
        {
            printf("1");
        }
        else if(a<0&&b>0)
        {
            printf("2");
        }
        else if(a<0&&b<0)
        {
            printf("3");
        }
        else if(a>0&&b<0)
        {
            printf("4");
        }
    }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
