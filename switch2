#include <stdio.h>
#include<math.h>


int main() {
    // Write C code here
    //switch
    int a,b;
    scanf("%d%d",&a,&b);
    int ch;
    scanf("\n%d",&ch);
    switch(ch)
    {
        case 1:
        printf(" addition-%d",a+b);
        break;
        case 2:
        printf("subtraction-%d",a-b);
       break;
        case 3:
        printf("multiplication-%d",a*b);
        break;
        case 4:
        printf("division-%d",a/b);
        break;
       
        default:
        printf("Invalid");
    }
    return 0;
}
