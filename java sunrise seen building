import java.util.*;
class Test4
{
	public static void main(String args[])
	{
		Scanner s=new Scanner(System.in);
		int n=s.nextInt();
		int count=1;
		int a[]=new int[n];
		for(int i=0;i<n;i++)
		{
			a[i]=s.nextInt();
		}
		int large=a[0];
		for(int i=0;i<n;i++)
		{
			if(large<a[i])
			{
			count++;	
			}
		}
		System.out.print(count);
		
	}
}
